{
  "name": "@dckit/ui",
  "version": "0.1.0",
  "description": "Components library based on material-ui",
  "repository": "https://github.com/ambroseus/dckit",
  "keywords": [
    "typescript",
    "redux",
    "tsdx",
    "redux-saga",
    "immer",
    "crud",
    "toolkit",
    "ducks-pattern",
    "dck",
    "react-redux",
    "react-hooks-redux"
  ],
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "module": "dist/ui.esm.js",
  "types": "dist/index.d.ts",
  "typings": "dist/index.d.ts",
  "author": "Eugene Samonenko <esamonenko@agilevision.io>",
  "license": "MIT",
  "scripts": {
    "build": "tsdx build",
    "start": "pm2 --name @dckit/ui start tsdx -- watch",
    "dev": "pm2 --name @dckit/ui start tsdx -- watch --onFirstSuccess 'yarn dev:next'",
    "dev:next": "cd ../forms && yarn dev",
    "lint": "tsdx lint src && tsc --noEmit",
    "prepare": "yarn build && yarn test:coverage",
    "test": "CI=true tsdx test --env=jest-environment-jsdom-sixteen --color",
    "test:coverage": "CI=true tsdx test  --coverage --env=jest-environment-jsdom-sixteen --color ",
    "format": "prettier --write 'src/**/*.ts*'",
    "format:check": "prettier -l 'src/**/*.ts*'",
    "update": "ncu -u"
  },
  "devDependencies": {
    "@types/jest": "25.2.1",
    "@types/node": "13.11.1",
    "@types/material-ui": "0.21.7",
    "@types/react-router": "5.1.5",
    "@types/react-router-dom": "5.1.4",
    "@testing-library/react": "10.0.2",
    "eslint-config-react-app": "5.2.1",
    "eslint-config-unobtrusive": "1.2.5",
    "eslint-plugin-react": "7.19.0",
    "jest-environment-jsdom-sixteen": "1.0.3",
    "prettier": "1.19.1",
    "tsdx": "0.13.1",
    "tslib": "1.11.1",
    "typescript": "3.8.3"
  },
  "dependencies": {
    "@dckit/routes": "*",
    "@material-ui/core": "4.9.10",
    "@material-ui/icons": "4.9.1",
    "@material-ui/lab": "4.0.0-alpha.49",
    "clsx": "1.1.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-router": "5.1.2",
    "react-router-dom": "5.1.2",
    "react-teleporter": "2.1.1"
  },
  "peerDependencies": {
    "@material-ui/core": "4.9.10",
    "@material-ui/icons": "4.9.1",
    "@material-ui/lab": "4.0.0-alpha.49",
    "clsx": "1.1.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-router": "5.1.2",
    "react-router-dom": "5.1.2",
    "react-teleporter": "2.1.1"
  },
  "browserslist": {
    "production": [
      ">2%",
      "not dead",
      "not ie > 0",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.{ts,tsx}",
      "!<rootDir>/src/index.{ts,tsx}",
      "!<rootDir>/src/**/*.(spec|test|stories).{ts,tsx}",
      "!<rootDir>/src/__tests__/*",
      "!<rootDir>/src/**/*.d.{ts,tsx}"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "files": [
    "dist",
    "src"
  ],
  "sideEffects": false
}
