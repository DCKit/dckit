{
  "name": "@dckit/ui",
  "version": "0.1.0",
  "description": "Components library based on material-ui",
  "repository": "https://github.com/ambroseus/dckit",
  "keywords": [
    "typescript",
    "redux",
    "tsdx",
    "redux-saga",
    "immer",
    "crud",
    "toolkit",
    "ducks-pattern",
    "dck",
    "react-redux",
    "react-hooks-redux"
  ],
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/dckit-ui.esm.js",
  "module": "dist/dckit-ui.esm.js",
  "types": "dist/index.d.ts",
  "author": "Eugene Samonenko <esamonenko@agilevision.io>",
  "license": "MIT",
  "devDependencies": {
    "@ambroseus/tsdx": "^0.12.3",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.7.4",
    "@babel/plugin-proposal-optional-chaining": "^7.7.5",
    "babel-plugin-transform-imports": "^2.0.0",
    "@types/jest": "^24.0.25",
    "@types/node": "^13.1.4",
    "@types/material-ui": "^0.21.7",
    "@types/react-redux": "^7.1.5",
    "@types/react-router": "^5.1.3",
    "@types/react-router-dom": "^5.1.3",
    "@testing-library/react": "^9.4.0",
    "eslint-config-react-app": "5.1.0",
    "prettier": "1.19.1",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "tslib": "^1.10.0",
    "typescript": "3.7.4"
  },
  "dependencies": {
    "@dckit/store": "*",
    "@material-ui/core": "^4.8.3",
    "@material-ui/icons": "^4.5.1",
    "@reduxjs/toolkit": "^1.2.1",
    "clsx": "^1.0.4",
    "react-hook-form": "^4.4.5",
    "react-redux": "^7.1.3",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-teleporter": "^2.0.1",
    "redux": "^4.0.5"
  },
  "peerDependencies": {
    "@material-ui/core": "^4.8.3",
    "@material-ui/icons": "^4.5.1",
    "@reduxjs/toolkit": "^1.2.1",
    "clsx": "^1.0.4",
    "react-hook-form": "^4.4.5",
    "react-redux": "^7.1.3",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-teleporter": "^2.0.1",
    "redux": "^4.0.5"
  },
  "scripts": {
    "build": "tsdx build --format esm --name dckit-ui",
    "start": "tsdx watch --format esm --name dckit-ui",
    "dev": "pm2 --name @dckit/ui start tsdx --wait-ready --listen-timeout 5000 -- watch --format esm --name dckit-ui",
    "format": "prettier --write \"src/*.ts\" \"src/**/*.ts\" \"**/*.md\"",
    "format:check": "prettier --list-different \"src/*.ts\" \"src/**/*.ts\" \"**/*.md\"",
    "lint": "tsdx lint src && tsc --noEmit",
    "prepare": "npm run test:coverage && npm run build",
    "test": "CI=true tsdx test --color",
    "test:coverage": "CI=true tsdx test --color --coverage"
  },
  "jest": {
    "moduleNameMapper": {
      "@/(.*)$": "<rootDir>/src/$1"
    }
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "files": [
    "dist",
    "src"
  ],
  "sideEffects": false
}
